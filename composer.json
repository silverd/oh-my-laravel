{
    "name": "silverd/oh-my-laravel",
    "type": "library",
    "require": {
        "beyondcode/laravel-dump-server": "^1.9",
        "guzzlehttp/guzzle": "^7.8",
        "predis/predis": "^2.2",
        "laravel/horizon": "^5.0",
        "mnabialek/laravel-sql-logger": "^2.2",
        "monolog/monolog": "^3.0",
        "symfony/mailer": "^6.2 || ^7.0",
        "laravel/framework": "^10.0",
        "ext-bcmath": "*",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Silverd\\OhMyLaravel\\": "src/"
        },
        "files": [
            "src/constants.php",
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Silverd\\OhMyLaravel\\ServiceProvider",
                "Silverd\\OhMyLaravel\\Providers\\AppServiceProvider",
                "Silverd\\OhMyLaravel\\Providers\\AuthServiceProvider",
                "Silverd\\OhMyLaravel\\Providers\\LogServiceProvider",
                "Silverd\\OhMyLaravel\\Providers\\QueueServiceProvider",
                "Silverd\\OhMyLaravel\\Providers\\HorizonServiceProvider"
            ],
            "aliases": {
            },
            "dont-discover": [
                "mnabialek/laravel-sql-logger"
            ]
        }
    },
    "minimum-stability" : "stable",
    "authors": [
        {
            "name": "silverd",
            "email": "silverd29@gmail.com"
        }
    ]
}
