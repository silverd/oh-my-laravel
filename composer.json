{
    "name": "silverd/oh-my-laravel",
    "type": "library",
    "require": {
        "beyondcode/laravel-dump-server": "^1.4",
        "guzzlehttp/guzzle": "^6.3 || ^7.0.1 || ^7.2.0",
        "predis/predis": "^1.1",
        "laravel/horizon": "^4.0 || ^5.0",
        "mnabialek/laravel-sql-logger": "^2.2",
        "fruitcake/laravel-cors": "^1.0 || ^2.0",
        "maatwebsite/excel": "^3.1",
        "monolog/monolog": "^2.8",
        "symfony/mailer": "^5.4"
    },
    "autoload": {
